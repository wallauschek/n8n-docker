# Acesso b√°sico ao n8n
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# Host de acesso externo
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# Webhook
N8N_TUNNEL_SUBDOMAIN=n8n-dev

